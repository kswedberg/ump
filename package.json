    "chalk": "^2.4.2",
    "fs-extra": "^1.0.0",
    "git-config": "0.0.7",
    "glob": "^7.1.4",
    "inquirer": "^7.0.0",
    "nomnom": "^1.8.1",
    "rc": "^1.2.8",
    "semver": "^6.3.0",
    "update-notifier": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
